{
  "require": {
    "craftcms/ckeditor": "3.14.1",
    "craftcms/cms": "^4.0.0",
    "craftcms/feed-me": "^5.14.0",
    "dolphiq/redirect": "2.0.1",
    "nystudio107/craft-seomatic": "4.1.19",
    "presseddigital/linkit": "4.0.4.1",
    "spacecatninja/imager-x": "4.5.3",
    "spicyweb/craft-neo": "4.4.7",
    "verbb/field-manager": "3.0.9",
    "verbb/formie": "2.2.6",
    "verbb/super-table": "3.0.16",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.3"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
