{
  "require": {
    "barrelstrength/sprout-forms": "3.14.0",
    "craftcms/cms": "^3.7.26",
    "craftcms/feed-me": "^4.8.1",
    "nystudio107/craft-seomatic": "3.5.22",
    "sebastianlenz/linkfield": "2.0.0-rc.2",
    "spacecatninja/imager-x": "v3.6.8",
    "verbb/field-manager": "2.2.5",
    "verbb/super-table": "2.7.5.1",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.3"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}